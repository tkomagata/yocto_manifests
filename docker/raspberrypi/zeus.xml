<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="yoctoproject" fetch="git://git.yoctoproject.org" />
	<remote name="openembedded" fetch="git://git.openembedded.org" />
	<remote name="github" fetch="git://github.com" />

	<default remote="yoctoproject" revision="zeus" sync-j="8" sync-c="true" />

	<!-- Yocto Layers -->
	<project name="poky" path="poky" />
	<project remote="yoctoproject" name="meta-virtualization" path="meta-virtualization" />
	<project remote="openembedded" name="meta-openembedded" path="meta-openembedded" />
	<project name="meta-raspberrypi" path="meta-raspberrypi" />
	<project remote="github" name="tkomagata/meta-docker" path="meta-docker"/>
</manifest>
