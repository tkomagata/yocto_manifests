<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="github" fetch="git://github.com" />
	<remote name="yoctoproject" fetch="git://git.yoctoproject.org" />
	<remote name="openembedded" fetch="git://git.openembedded.org" />
	<remote name="linaro" fetch="git://git.linaro.org" />

	<default remote="github" revision="sumo" sync-j="8" sync-c="true" />

	<!-- Yocto Layers -->
	<project remote="yoctoproject" name="poky" path="poky" />
	<project remote="openembedded" name="meta-openembedded" path="meta-openembedded" />
	<project remote="linaro" name="openembedded/meta-linaro" path="meta-linaro" />
	<project name="renesas-rcar/meta-renesas" revision="sumo-dev" path="meta-renesas" />
</manifest>
